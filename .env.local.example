# 백엔드 API 사용 여부
# true로 설정하면 API 서버 사용, false면 localStorage만 사용
NEXT_PUBLIC_USE_BACKEND=false

# Supabase 설정 (선택사항)
# Supabase를 사용하려면 아래 값을 설정하세요
# https://supabase.com 에서 프로젝트 생성 후 API 키 복사

# Supabase 프로젝트 URL
NEXT_PUBLIC_SUPABASE_URL=your-project-url.supabase.co

# Supabase Anon/Public Key
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
